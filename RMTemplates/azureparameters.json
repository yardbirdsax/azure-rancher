{
    "$schema": "https://schema.management.azure.com/schemas/2015-01-01/deploymentParameters.json#",
    "contentVersion": "1.0.0.0",
    "parameters": {
      "adminUsername":{
        "value": "azureadmin"
      },
      "sshKey":{
          "metadata": {
              "description":"The SSH public key to use with the VMs."
          },
          "value": ""
      },
      "virtualMachineName":{
        "value": "USE2-RNKUB",
        "metadata": {
          "description":"The first part of the virtual machine name. Full names will use this plus a numerical incrementing identifier based on the number of VMs to provision."
        }
      },
      "virtualNetworkResourceGroupName":{
        "value":"USE2-LOGAKSCLUS01"
      },
      "virtualNetworkName":{
        "value":"USE2-LOGAKSCLUS01"
      },
      "subnetName":{
        "value":"rancher"
      },
      "loadBalancerName":{
        "value":"LB-USE2-Rancher"
      },
      "backendPoolName":{
        "value":"KubeAPIServer-BackendPool"
      },
      "recoveryVaultName":{
        "value": "none"
      },
      "serverCount":{
        "value": 3
      }
    }
}